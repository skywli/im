<!-- dispatch configuration type is a important parameter,the value must be one of(monitor,login,conn,dispatch,msg,loadbalance,broadcast)-->
<dispatch  type="dispatch">
	 <!-- IP/port the monitor is waiting for connections on -->
	<monitor_ip>127.0.0.1</monitor_ip>
	<monitor_port>8802</monitor_port>
	
	  <!-- IP and port to listen for incoming dispatch connections on -->
	<ip>127.0.0.1</ip>
	<port>8010</port>
	<!-- dispatch will connect service -->
	<conn_type>
		<type>monitor</type>
		<type>conn</type>
	</conn_type>
	<!-- dispatch whether runing on backgroud ,the value is off/on -->
	<daemon>on</daemon>
		<!-- specify log level 
	This should be one of
	debug:1
	trace:2
	info:3
	warn:4
	error:5
	//default debug
	-->
	<loglevel>1</loglevel>  
</dispatch>
	
